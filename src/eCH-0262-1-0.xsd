								</xs:element>
								<xs:element name="unit">
									<xs:simpleType>
										<xs:restriction base="eCH-0261:unitType">
											<xs:enumeration value="m3"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
							</xs:sequence>
						</xs:restriction>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
